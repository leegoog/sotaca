<% title "Product Likes" %>

<table>
  <tr>
    <th>User</th>
    <th>Product</th>
  </tr>
  <% for product_likes in @product_likes %>
    <tr>
      <td><%= product_likes.user_id %></td>
      <td><%= product_likes.product_id %></td>
      <td><%= link_to "Show", product_likes %></td>
      <td><%= link_to "Edit", edit_product_likes_path(product_likes) %></td>
      <td><%= link_to "Destroy", product_likes, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Product Likes", new_product_likes_path(product_likes) %></p>
